{
  "name": "supersdr-webhook-normalizer",
  "version": "1.0.0",
  "description": "Sistema de normalização de webhooks de múltiplos provedores de WhatsApp",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "db:migrate": "psql -d supersdr -f src/database/schema.sql"
  },
  "keywords": [
    "whatsapp",
    "webhook",
    "normalization",
    "meta",
    "evolution-api",
    "z-api",
    "adapter-pattern"
  ],
  "author": "Candidato",
  "license": "MIT",
  "dependencies": {
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
